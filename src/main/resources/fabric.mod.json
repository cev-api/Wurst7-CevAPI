{
  "schemaVersion": 1,
  "id": "${modId}",
  "version": "${version}",
  "name": "${modName}",
  "description": "${modDescription}",
  "authors": [
    "Alexander01998",
    "CevAPI"
  ],
  "contact": {
    "homepage": "${homepage}",
    "sources": "${sources}",
    "issues": "${issues}"
  },
  "license": "GPL-3.0-or-later",
  "icon": "assets/wurst/icon.png",
  "environment": "client",
  "entrypoints": {
    "main": [
      "net.wurstclient.WurstInitializer"
    ]
  },
  "mixins": [
    "wurst.mixins.json"
  ],
  "accessWidener": "wurst.accesswidener",
  "depends": {
    "fabricloader": ">=0.17.3",
    "fabric-api": ">=0.140.0",
    "minecraft": "~1.21.11-beta.1",
    "java": ">=21"
  },
  "suggests": {
    "mo_glass": "*"
  },
  "breaks": {
    "wi_zoom": "*",
    "wi-zoom": "*",
    "vulkanmod": "*"
