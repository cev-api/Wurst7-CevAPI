  "custom": {
    "modmenu": {
      "links": {
        "GitHub": "https://github.com/cev-api/Wurst7-CevAPI/",
        "modmenu.twitter": "https://www.wurstclient.net/modmenu-twitter/",
        "modmenu.wiki": "https://www.wurstclient.net/modmenu-wiki/",
        "Forum": "https://www.wurstclient.net/modmenu-forum/"
